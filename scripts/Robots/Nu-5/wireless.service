[Unit]
Description=B-Human WLAN network configuration
After=dbus.service
Before=network.target
Wants=network.target
Conflicts=wpa_supplicant.service

[Service]
Type=simple
ExecStartPre=/sbin/ifconfig wlan0 up
ExecStart=/usr/sbin/wpa_supplicant -iwlan0 -Dnl80211 -c/home/nao/team_nust/Profiles/teamNUST-SPL
ExecStartPost=/bin/sleep 1
ExecStartPost=/sbin/ifconfig wlan0 10.0.30.5 netmask 255.255.0.0
ExecStopPost=/sbin/ifconfig wlan0 down

[Install]
WantedBy=multi-user.target